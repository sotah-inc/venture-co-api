  "dependencies": {
    "@google-cloud/firestore": "^2.2.1",
    "@sotah-inc/api": "^1.0.10",
    "npm-run-all": "^4.1.5",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@blueprintjs/tslint-config": "^1.8.0",
    "chokidar-cli": "^1.2.2",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.17.0",
